class Solution {
public:
	string predictPartyVictory(string senate) {
		int n = senate.size();
		queue<int> dire,radiant;
		for(int i=0;i<n;i++){
			if(senate[i]=='D')dire.push(i);
			else radiant.push(i);
		}
		while(!dire.empty() && !radiant.empty()){
			int d = dire.front();dire.pop();            
			int r = radiant.front();radiant.pop();
			if(d<r)dire.push(d+n); 
			else radiant.push(r+n); 
		}
		return dire.empty()?"Radiant":"Dire"; 
	}
};
